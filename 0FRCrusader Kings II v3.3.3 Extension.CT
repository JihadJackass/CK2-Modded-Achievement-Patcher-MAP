          <AssemblerScript>//=========================================
[ENABLE]
//=========================================
label(CHP1)
label(CHP2)
label(CHP3)
registersymbol(CHP1)
registersymbol(CHP2)
registersymbol(CHP3)

AobScanModule(aob1, $process, ?? ?? 48 8B D9 80 B8 00 05 00 00 00 75 ??) //show console
AobScanModule(aob2, $process, 8B 1D ?? ?? ?? ?? 4C 39 AB F0 04 00 00 ??) //enable console
AobScanModule(aob3, $process, E8 ?? ?? ?? ?? C6 40 61 00 0F B6 45 A7 88) //uncorrupt save files

//=========================================
// Actual script
//=========================================

//Removes the check for showing the console
aob1+5:
CHP1:
 nop
 nop
 nop
 nop
 nop
 cmp eax,eax

//Removes the check for ironman mode
aob2+6:
CHP2:
 nop
 nop
 nop
 nop
 nop
 cmp eax,eax

aob3+5:
CHP3:
 mov byte ptr [eax+35],01

//=========================================
[DISABLE]
//=========================================
CHP1:
//cmp byte ptr [rax+00000500],00
 db 80 B8 00 05 00 00 00
CHP2:
//cmp [rbx+000004F0],r13
 db 4C 39 AB F0 04 00 00
CHP3:
//mov byte ptr [rax+61],00
 db C6 40 61 00

unregistersymbol(CHP)
unregistersymbol(CHP2)
unregistersymbol(CHP3)
</AssemblerScript>
