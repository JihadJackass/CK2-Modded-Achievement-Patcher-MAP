          <Color>C08000</Color>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>//=========================================
[ENABLE]
//=========================================

AobScanModule(aob_ironman,        $process, 0F B6 83 F9 02 00 00 48)
AobScanModule(aob_ruler_designer, $process, C6 40 62 01 48 8B 8B 10 6C 00 00)
AobScanModule(aob_savegame_check, $process, 88 46 61 48 8B 03 )
AobScanModule(aob_checksum_check, $process, 88 4F 63 48 8B 46 30 )

alloc(achievement_script,8192,$process))

label(set_ironman)
label(return_ironman)
label(set_ruler_designer)
label(return_ruler_designer)
label(set_savegame_check)
label(return_savegame_check)
label(set_checksum_check)
label(return_checksum_check)

label(base_ironman_pointer)
label(base_achievement_pointer)

registersymbol(set_ironman)
registersymbol(return_ironman)
registersymbol(set_ruler_designer)
registersymbol(return_ruler_designer)
registersymbol(set_savegame_check)
registersymbol(return_savegame_check)
registersymbol(set_checksum_check)
registersymbol(return_checksum_check)

registersymbol(base_ironman_pointer)
registersymbol(base_achievement_pointer)

registersymbol(aob_ironman)
registersymbol(aob_ruler_designer)
registersymbol(aob_savegame_check)
registersymbol(aob_checksum_check)

//=========================================
// Actual script
//=========================================
achievement_script:

set_ironman:
  mov [base_ironman_pointer], rbx
  mov [rbx+000002F9], 1
  movzx eax,byte ptr [rbx+000002F9]
 jmp return_ironman

set_ruler_designer:
  mov [base_achievement_pointer], rax
  mov byte ptr [rax+61],01                 //Set Save file unaltered = 1
  mov byte ptr [rax+62],00                 //Set Ruler designer used = 0
  mov byte ptr [rax+63],01                 //Set Checksum is vanilla = 1
  mov byte ptr [rax+65],01                 //Set Steam is enabled    = 1
  mov rcx,[rbx+00006C10]
 jmp return_ruler_designer

set_savegame_check:
  mov [base_achievement_pointer], rsi
  mov byte ptr [rsi+61],01                 //Set Save file unaltered = 1
  mov byte ptr [rsi+62],00                 //Set Ruler designer used = 0
  mov byte ptr [rsi+63],01                 //Set Checksum is vanilla = 1
  mov byte ptr [rsi+65],01                 //Set Steam is enabled    = 1
  mov rax,[rbx]
 jmp return_savegame_check

set_checksum_check:
  mov [base_achievement_pointer], rdi
  mov byte ptr [rdi+61],01                 //Set Save file unaltered = 1
  mov byte ptr [rdi+62],00                 //Set Ruler designer used = 0
  mov byte ptr [rdi+63],01                 //Set Checksum is vanilla = 1
  mov byte ptr [rdi+65],01                 //Set Steam is enabled    = 1
  mov rax,[rsi+30]
 jmp return_checksum_check

//=========================================
// Variables
//=========================================

base_ironman_pointer:
  dq 0

base_achievement_pointer:
  dq 0

//=========================================
// Hack points
//=========================================

aob_ironman:
  jmp set_ironman
  nop 2
return_ironman:

aob_ruler_designer:
  jmp set_ruler_designer
  nop 6
return_ruler_designer:

aob_savegame_check:
  jmp set_savegame_check
  nop
return_savegame_check:

aob_checksum_check:
  jmp set_checksum_check
  nop 2
return_checksum_check:

//=========================================
[DISABLE]
//=========================================

aob_ironman:
  // movzx eax,byte ptr [rbx+000002F9]
  db 0F B6 83 F9 02 00 00

aob_ruler_designer:
  // mov byte ptr [rax+62],01
  // mov rcx,[rbx+00006C10]
  db C6 40 62 01 48 8B 8B 10 6C 00 00

aob_savegame_check:
  // mov [rsi+61],al
  // mov rax,[rbx]
  db 88 46 61 48 8B 03

aob_checksum_check:
  // mov [rdi+63],cl
  // mov rax,[rsi+30]
  db 88 4F 63 48 8B 46 30

unregistersymbol(set_ironman)
unregistersymbol(return_ironman)
unregistersymbol(set_ruler_designer)
unregistersymbol(return_ruler_designer)
unregistersymbol(set_savegame_check)
unregistersymbol(return_savegame_check)
unregistersymbol(set_checksum_check)
unregistersymbol(return_checksum_check)

unregistersymbol(base_ironman_pointer)
unregistersymbol(base_achievement_pointer)

unregistersymbol(aob_ironman)
unregistersymbol(aob_ruler_designer)
unregistersymbol(aob_savegame_check)
unregistersymbol(aob_checksum_check)

dealloc(achievement_script)
</AssemblerScript>
